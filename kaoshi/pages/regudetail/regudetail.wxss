/* pages/regudetail/regudetail.wxss */
@import"../../wxParse/wxParse.wxss";
.dyn_item{margin-top:20rpx;background: #fff;overflow: hidden;}
.regu_item_head{
  padding:0 30rpx;
  border-bottom: 1rpx solid #ccc;
  margin-bottom: 30rpx;
}
.regu_item_head .regu_item_head_tit{
  font-size:38rpx;
  color:#333333;
}
.regu_item_head_stat{
  padding:24rpx 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.regu_item_head_stat text{
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  -ms-flex: 1;
  flex: 1;
  width: 100%;
  text-align: center;
  color:#999999;
  font-size:24rpx;
}
.regu_item_head_stat text.left{
  text-align: left;
}
.regu_item_head_stat text.right{
  text-align: right;
}
.dyn_item_tit{padding:30rpx;}
.dyn_item_cont{padding:0 30rpx 30rpx 30rpx;}
.dyn_tit_left{float:left;width:80rpx;}
.dyn_tit_right{float:right;width:50rpx;}
.dyn_tit_mid{margin-left:100rpx;margin-right:63rpx;}
.dyn_tit_left image{
  width:80rpx;
  height:80rpx;
  border-radius: 80rpx;
}
.dyn_tit_right image{
  width:43rpx;
}
.dyn_tit_nickname{
  font-size:30rpx;
  line-height: 55rpx;
}
.dyn_tit_crttime{
  font-size:24rpx;
  color:#999999;
  line-height: 24rpx;
}
.dyn_item_cont{
  font-size:28rpx;
  line-height: 36rpx;
}
.dyn_item_cont image{
  max-width: 100% !important;
  height: auto;
}

page{
  padding-bottom: 118rpx;
}

.reguoper{
  position: fixed;
  bottom: 0;
  left:0;
  right:0;
  height:88rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  /* background: #fff;
  box-shadow: 0 0 2px rgba(0, 0, 0, 0.1); */
  background: #3eccb3;
}
.reguoper_item{
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  -ms-flex: 1;
  flex: 1;
  text-align: center;
  height:88rpx;
  line-height: 88rpx;
  color:#fff;
  font-size:30rpx;
}
.reguoper_item view{
  border-left: 1rpx solid #fff;
  height:30rpx;
  line-height:30rpx;
  text-align: center;
  margin-top:35rpx;
}
.reguoper_item view.noborder{
  border-left:none;
}
.reguoper_item image{
  width:36rpx;
  height:36rpx;
  vertical-align: middle;
  margin-right:10rpx;
}
.reguoper .reguoper_item.active{
  background: #3eccb3;
  color:#fff;
}
.moreoper{
  position: fixed;
  bottom:176rpx;
  right:40rpx;
}
.moreoper_box{
  position: relative;
}
image.more_button{
  width:100rpx;
  height:100rpx;
  border-radius: 100rpx;
  position: absolute;
  right:0;
  bottom:0;
}
.moreoper .more_list{
  background: rgba(0,0,0,0.7);
  color:#fff;
  padding:10rpx 60rpx 10rpx 60rpx;
  border-radius: 100rpx;
  display: none;
  height:80rpx;
}
.moreoper .more_list.active{
  display: block;
}
.moreoper .more_list .more_list_item{
  display: inline-block;
  text-align: center;
  margin-right:80rpx;
}
.moreoper .more_list .more_list_item button{
  background: transparent;
  padding-left:0;
  padding-right:0;
  border:none;
  color:#fff;
  padding-top:0;
  padding-bottom: 0;
  margin-left:0;
  margin-right: 0;
}

.moreoper .more_list button.more_list_item::after{
  display: none;
}
.moreoper .more_list .more_list_item image{
  width:42rpx;
  height:42rpx;
  display: block;
  margin:0 auto;
}
.moreoper .more_list .more_list_item  text{
  font-size:26rpx;
  display: block;
  height:38rpx;
  line-height: 38rpx;
}
.fix_mask {
